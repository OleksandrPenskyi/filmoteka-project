<div class="paginator">
  <div class="paginator-page paginator-previous is-hidden"><svg width="16" height="16" viewBox="0 0 16 16" fill="none"
      xmlns="http://www.w3.org/2000/svg">
      <path d="M12.6667 8H3.33337" stroke="black" stroke-width="1.33333" stroke-linecap="round"
        stroke-linejoin="round" />
      <path d="M8.00004 12.6667L3.33337 8.00004L8.00004 3.33337" stroke="black" stroke-width="1.33333"
        stroke-linecap="round" stroke-linejoin="round" />
    </svg></div>
  <div class="paginator-page firstItem is-hidden">1</div>
  <div class="paginator-page dots is-hidden">...</div>

  <div class="pages">
    <div class="paginator-page prevprev is-hidden">-1</div>
    <div class="paginator-page prev is-hidden">0</div>
    <div class="paginator-page paginator-current">1</div>
    <div class="paginator-page next">2</div>
    <div class="paginator-page nextnext">3</div>
  </div>
  <div class="paginator-page lastdots">...</div>

  <div class="paginator-page lastItem">{{totalPages}}</div>
  <div class="paginator-page paginator-next"><svg width="16" height="16" viewBox="0 0 16 16" fill="none"
      xmlns="http://www.w3.org/2000/svg">
      <path d="M3.33329 8H12.6666" stroke="black" stroke-width="1.33333" stroke-linecap="round"
        stroke-linejoin="round" />
      <path d="M7.99996 12.6667L12.6666 8.00004L7.99996 3.33337" stroke="black" stroke-width="1.33333"
        stroke-linecap="round" stroke-linejoin="round" />
    </svg></div>
</div>