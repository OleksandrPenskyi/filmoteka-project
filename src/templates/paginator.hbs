<div class="paginator">
  <div class="paginator-page paginator-previous is-hidden pagination-btn">
    <?xml version="1.0" encoding="UTF-8"?>
    <svg class="pagination-svg" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
      width="16pt" height="16pt" viewBox="0 0 16 16" version="1.1">
      <g id="surface1">
        <path class="pagination-svg"
          style="fill:none;stroke-width:1.33333;stroke-linecap:round;stroke-linejoin:round;stroke-opacity:1;stroke-miterlimit:4;"
          d="M 12.667969 8 L 3.332031 8 " />
        <path class="pagination-svg"
          style="fill:none;stroke-width:1.33333;stroke-linecap:round;stroke-linejoin:round;stroke-opacity:1;stroke-miterlimit:4;"
          d="M 8 12.667969 L 3.332031 8 L 8 3.332031 " />
      </g>
    </svg>
  </div>
  <div class="paginator-page firstItem is-hidden">1</div>
  <div class="paginator-page dots is-hidden">...</div>

  <div class="pages">
    <div class="paginator-page prevprev is-hidden">-1</div>
    <div class="paginator-page prev is-hidden">0</div>
    <div class="paginator-page paginator-current">1</div>
    <div class="paginator-page next">2</div>
    <div class="paginator-page nextnext">3</div>
  </div>
  <div class="paginator-page lastdots">...</div>

  <div class="paginator-page lastItem">{{totalPages}}</div>
  <div class="paginator-page paginator-next pagination-btn">
    <?xml version="1.0" encoding="UTF-8"?>
    <svg class="pagination-svg" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
      width="16pt" height="16pt" viewBox="0 0 16 16" version="1.1">
      <g id="surface1">
        <path class="pagination-svg"
          style="fill:none;stroke-width:1.33333;stroke-linecap:round;stroke-linejoin:round;stroke-opacity:1;stroke-miterlimit:4;"
          d="M 3.332031 8 L 12.667969 8 " />
        <path class="pagination-svg"
          style="fill:none;stroke-width:1.33333;stroke-linecap:round;stroke-linejoin:round;stroke-opacity:1;stroke-miterlimit:4;"
          d="M 8 12.667969 L 12.667969 8 L 8 3.332031 " />
      </g>
    </svg>
  </div>
</div>